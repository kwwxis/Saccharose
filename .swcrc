[
  {
    "test": ".tsx?$",
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": true,
        "dynamicImport": true,
        "privateMethod": false,
        "functionBind": false,
        "exportDefaultFrom": false,
        "exportNamespaceFrom": false,
        "decorators": true,
        "decoratorsBeforeExport": false,
        "topLevelAwait": true,
        "importMeta": true
      },
      "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true
      }
    }
  },
  {
    "test": ".jsx?$",
    "jsc": {
      "parser": {
        "syntax": "ecmascript",
        "jsx": true,
        "dynamicImport": true,
        "privateMethod": false,
        "functionBind": false,
        "exportDefaultFrom": false,
        "exportNamespaceFrom": false,
        "decorators": true,
        "decoratorsBeforeExport": false,
        "topLevelAwait": true,
        "importMeta": true
      },
      "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true
      }
    }
  }
]
