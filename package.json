  "scripts": {
    "backend:build": "node --no-warnings=ExperimentalWarning --loader ts-node/esm build.ts",
    "ts-serve": "node --no-warnings=ExperimentalWarning --loader ts-node/esm build.ts && node .",
    "ts-serve:dev": "nodemon",
    "rspack:prod": "cross-env NODE_OPTIONS=\"--no-warnings=ExperimentalWarning --loader ts-node/esm\" rspack build --config rspack.prod.ts --env prod",
    "rspack:prod:doctor": "cross-env NODE_OPTIONS=\"--no-warnings=ExperimentalWarning --loader ts-node/esm\" RSDOCTOR=true rspack build --config rspack.prod.ts --env prod",
    "rspack:dev": "cross-env NODE_OPTIONS=\"--no-warnings=ExperimentalWarning --loader ts-node/esm\" rspack build --config rspack.dev.ts --env dev",
    "rspack:dev:doctor": "cross-env NODE_OPTIONS=\"--no-warnings=ExperimentalWarning --loader ts-node/esm\" RSDOCTOR=true rspack build --config rspack.dev.ts --env dev",
    "rspack:dev:watch": "cross-env NODE_OPTIONS=\"--no-warnings=ExperimentalWarning --loader ts-node/esm\" rspack build --config rspack.dev.ts --watch --env dev",
    "build:dev": "npm-run-all --parallel backend:build rspack:dev",
    "build:prod": "npm-run-all --parallel backend:build rspack:prod",
    "start": "node .",
    "test": "jest"
  },
